<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:forms="sap.ui.layout.form">
	<forms:SimpleForm id="idSequenceData" labelSpanL="4" labelSpanM="4" labelSpanS="4" emptySpanL="0" emptySpanM="0" emptySpanS="0" maxContainerCols="2"
		layout="ResponsiveGridLayout" width="100%">
		<core:Title text="Sequence Data"/>
		<Label text="Sequence Number"/>
		<Input id="idSequenceNumberValueHelp" type="Text" showSuggestion="true" showValueHelp="true" valueHelpRequest="handleValueHelp"
			suggestionItems="{/ProductCollection}">
			<suggestionItems>
				<core:Item text="{Name}"/>
			</suggestionItems>
		</Input>
	</forms:SimpleForm>
</mvc:View>